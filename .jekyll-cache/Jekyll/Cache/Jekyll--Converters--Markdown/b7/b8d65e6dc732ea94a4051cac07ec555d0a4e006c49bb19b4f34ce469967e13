<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>class Person {
    
    var name: String! = nil
    
    init(name: String) {
        self.name = name
    }
    
    func sayHello() {
        print(self.name + ":")
    }
}

class American: Person {
    
    override func sayHello() {
        print(self.name + ": Hello")
    }
}

class Chinese: Person {
    
    override func sayHello() {
        print(self.name + ": ä½ å¥½")
    }
}

let jack = American(name: "Jack")
jack.sayHello()
</code></pre></div></div>
